<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link data-require="bootstrap@4.0.5" data-semver="4.0.5" rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script data-require="bootstrap@4.0.5" data-semver="4.0.5" src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.5/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="./../../js/listingScripts.js"></script>
    <script type="text/javascript" src="./../../js/userScripts.js"></script>
    <link rel="stylesheet" href="./../../css/style.css">



</head>

<body>

<p><a onclick="backToSearch()" href="/fa17g12/">Back to Search</a></p>

<!-- LISTING DESCRIPTION -->
<div class="data-table">
    <table border="1" cellpadding="7" cellspacing="7">
        <tr>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip Code</th>
        </tr>

        <% if(data.length){ %>
        <tr>
            <td><%= data[0].address %></td>
            <td><%= data[0].city %></td>
            <td><%= data[0].state %></td>
            <td><%= data[0].zip_code %></td>
        </tr>

        <% } else{ %>
        <tr>
            <td colspan="5">No Data</td>
        </tr>
        <% } %>

    </table>
</div>

<img alt="Pic" src="../<%= data[0].image %>"/>


<div id="carouselExampleSlidesOnly" data-interval="1500" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner" role="listbox">
        <% for(var i = 0; i < images.length; i++){ %>
        <% if(i === 0){ %>
          <div class="carousel-item active">
              <img style="height:100px;width:250px;" class="d-block img-fluid" src="../<%=images[i].images%>"/>
          </div>
        <%} else { %>
          <div class="carousel-item">
              <img style="height:100px;width:250px;" class="d-block img-fluid" src="../<%=images[i].images%>"/>
          </div>
        <% } %>
        <%} %>
    </div>
</div>

