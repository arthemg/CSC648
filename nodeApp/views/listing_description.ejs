<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <script type="text/javascript" src="./../../js/jquery-1.10.2.min.js"></script>
    <link rel="stylesheet" href="./../../css/style.css">
    <script type="text/javascript" src="./../../js/listingScripts.js"></script>
    <script type="text/javascript" src="./../../js/userScripts.js"></script>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
</head>

<body>

<p><a onclick="backToSearch()" href="/fa17g12/">Back to Search</a></p>

<!-- LISTING DESCRIPTION -->
<div class="data-table">
    <table border="1" cellpadding="7" cellspacing="7">
        <tr>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip Code</th>
        </tr>

        <% if(data.length){ %>
        <tr>
            <td><%= data[0].address %></td>
            <td><%= data[0].city %></td>
            <td><%= data[0].state %></td>
            <td><%= data[0].zip_code %></td>
        </tr>

        <% } else{ %>
        <tr>
            <td colspan="5">No Data</td>
        </tr>
        <% } %>

    </table>
</div>

<img alt="Pic" src="../<%= data[0].image %>"/>

<!-- MAP -->
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDYSUWFvpVbz3iuStuGYjf0Ca4QJ4TBEgs">
</script>

<div id="map"></div>



<div><strong>Contact Seller</strong></div>
<div class="container">

    <div class="row">

        <div class="col-lg-6 col-lg-offset-2">

            <!--<h1>Contact form Tutorial from <a href="http://bootstrapious.com">Bootstrapious.com</a></h1>-->

            <form method="post" action="" id="message-form" role="form" novalidate="true">
                <div class="form-group" class="controls" class="row">
                    <div class="col-md-28">
                        <label>Name:</label>
                        <input type="text" class="form-control" id="buyerName" onkeydown="return messageSellerEnter(event)">
                    </div>
                </div>
                <div class="form-group">
                    <label>Phone Number:</label>
                    <input type="text" class="form-control" id="buyerPhone" onkeydown="return messageSellerEnter(event)">
                </div>

                <div class="form-group">
                    <label>Email</label>
                    <input type="text" class="form-control" id="buyerEmail" onkeydown="return messageSellerEnter(event)">
                </div>
                <div class="form-group">
                    <label>Message:</label>
                    <input type="text" class="form-control" id="buyerMessage" onkeydown="return messageSellerEnter(event)">
                </div>

                <input type="button" class="btn btn-warning btn-lg" value="Submit" id="message-button"
                       onclick="messageSeller(<%= data[0].user_id %>)">
            </form>

        </div><!-- /.8 -->

    </div> <!-- /.row-->

</div>

<script type="text/javascript">
    initMap("<%= data[0].address %>, <%= data[0].city %>, <%= data[0].state %>, <%= data[0].zip_code %>")
</script>

</body>
</html>
